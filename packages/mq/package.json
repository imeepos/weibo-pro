{
  "name": "@sker/mq",
  "version": "0.0.0",
  "private": true,
  "description": "RabbitMQ message queue client with connection pooling, retry strategies, and RxJS support.",
  "keywords": ["rabbitmq", "mq", "message-queue", "amqp", "rxjs", "sker"],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "claude": "claude --dangerously-skip-permissions",
    "lint": "eslint . --max-warnings 0",
    "check-types": "tsc --noEmit"
  },
  "dependencies": {
    "@sker/core": "workspace:*",
    "amqplib": "^0.10.5",
    "dotenv": "^16.4.7",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@sker/eslint-config": "workspace:*",
    "@sker/typescript-config": "workspace:*",
    "@types/amqplib": "^0.10.5",
    "@types/node": "^22.15.3",
    "eslint": "^9.38.0",
    "tsup": "^8.4.0",
    "typescript": "5.9.2"
  }
}
