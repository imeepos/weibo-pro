{
  "name": "@sker/workflow-ast",
  "version": "0.0.0",
  "private": true,
  "description": "Abstract syntax tree definitions for workflow nodes.",
  "keywords": [
    "workflow",
    "ast",
    "nodes",
    "sker"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "sideEffects": true,
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "claude": "claude --dangerously-skip-permissions",
    "lint": "eslint . --max-warnings 0",
    "check-types": "tsc --noEmit"
  },
  "dependencies": {
    "@sker/core": "workspace:*",
    "@sker/nlp": "workspace:*",
    "@sker/workflow": "workspace:*"
  },
  "devDependencies": {
    "@sker/entities": "workspace:*",
    "@sker/eslint-config": "workspace:*",
    "@sker/typescript-config": "workspace:*",
    "@types/node": "^22.15.3",
    "eslint": "^9.38.0",
    "tsup": "^8.4.0",
    "typescript": "5.9.2"
  }
}