{
  "name": "@sker/workflow",
  "version": "0.0.0",
  "private": true,
  "description": "Workflow engine and orchestration utilities for Sker applications.",
  "keywords": ["workflow", "orchestration", "sker"],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "default": "./dist/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "claude": "claude --dangerously-skip-permissions",
    "lint": "eslint . --max-warnings 0",
    "check-types": "tsc --noEmit"
  },
  "dependencies": {
    "rxjs": "^7.8.1",
    "@sker/core": "workspace:*"
  },
  "devDependencies": {
    "@sker/eslint-config": "workspace:*",
    "@sker/typescript-config": "workspace:*",
    "@types/node": "^22.15.3",
    "eslint": "^9.38.0",
    "tsup": "^8.4.0",
    "typescript": "5.9.2"
  }
}