{
  "name": "@sker/entities",
  "version": "0.0.0",
  "private": true,
  "description": "TypeORM entities and database models for Sker applications.",
  "keywords": ["entities", "database", "typeorm", "sker"],
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "require": "./src/index.ts",
      "import": "./src/index.ts"
    },
    "./*": {
      "types": "./src/*.ts",
      "require": "./src/*.ts",
      "import": "./src/*.ts"
    },
    "./package.json": "./package.json"
  },
  "types": "./src/index.ts",
  "main": "./src/index.ts",
  "sideEffects": false,
  "scripts": {
    "claude": "claude --dangerously-skip-permissions",
    "lint": "eslint . --max-warnings 0",
    "check-types": "tsc --noEmit"
  },
  "dependencies": {
    "typeorm": "^0.3.20",
    "@sker/core": "workspace:*",
    "reflect-metadata": "^0.2.2"
  },
  "devDependencies": {
    "@sker/eslint-config": "workspace:*",
    "@sker/typescript-config": "workspace:*",
    "@types/node": "^22.15.3",
    "eslint": "^9.38.0",
    "typescript": "5.9.2"
  }
}