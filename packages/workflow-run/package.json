{
  "name": "@sker/workflow-run",
  "version": "0.0.0",
  "private": true,
  "description": "Backend runtime execution for workflow nodes.",
  "keywords": [
    "workflow",
    "runtime",
    "backend",
    "execution",
    "sker"
  ],
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    },
    "./*": {
      "types": "./src/*.ts",
      "import": "./src/*.ts"
    },
    "./package.json": "./package.json"
  },
  "types": "./src/index.ts",
  "main": "./src/index.ts",
  "sideEffects": false,
  "scripts": {
    "claude": "claude --dangerously-skip-permissions",
    "lint": "eslint . --max-warnings 0",
    "check-types": "tsc --noEmit"
  },
  "dependencies": {
    "@sker/core": "workspace:*",
    "@sker/entities": "workspace:*",
    "@sker/nlp": "workspace:*",
    "@sker/redis": "workspace:*",
    "@sker/workflow": "workspace:*",
    "@sker/workflow-ast": "workspace:*",
    "cheerio": "^1.1.2",
    "playwright": "^1.56.1"
  },
  "devDependencies": {
    "@sker/eslint-config": "workspace:*",
    "@sker/typescript-config": "workspace:*",
    "@types/node": "^22.15.3",
    "eslint": "^9.38.0",
    "typescript": "5.9.2"
  }
}
